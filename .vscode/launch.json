  "version": "0.2.0",
  "configurations": [
    {
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "name": "Launch localhost",
      "url": "https://localhost:3000/dashboard",
      "webRoot": "${workspaceFolder}",
      "pathMappings": [
        {
          "url": "webpack-internal:///(rsc)/app/dashboard/(overview)/page.tsx",
          "path": "${workspaceFolder}/app/dashboard/invoices/page.tsx"
        }
      ]
    }
  ]
}
